n    var x1 = x;\r\n    var y1 = y;\r\n\r\n    var x2 = x;\r\n    var y2 = y - height;\r\n\r\n    var x3 = x + width;\r\n    var y3 = y;\r\n\r\n    return new Triangle(x1, y1, x2, y2, x3, y3);\r\n};\r\n\r\nmodule.exports = BuildRight;\r\n"},"lineCount":null}},"error":null,"hash":"247ff209d825274c6bf0825857345bd2","cacheData":{"env":{}}}